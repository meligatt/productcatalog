{
  "name": "productcatalog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run tsc",
    "start": "node ./dist/server.js",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
    "lint": "yarn run eslint . --ext=.ts",
    "migration:run": "yarn run ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "yarn run ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^1.3.9",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3",
    "pino-pretty": "^4.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "dependencies": {
    "@adonisjs/ace": "^6.8.4",
    "@adonisjs/core": "^5.0.0-preview.5",
    "@adonisjs/fold": "^6.3.4",
    "@adonisjs/lucid": "^7.6.3",
    "@adonisjs/session": "^2.3.3",
    "@adonisjs/view": "^1.0.13",
    "koa": "^2.11.0",
    "koa-router": "^8.0.8",
    "pg": "^8.0.0",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.16",
    "typeorm": "^0.2.24"
  }
}
